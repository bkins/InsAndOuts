<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="InsAndOuts.Views.ManageDataView"
             Shell.TabBarIsVisible="False"
             >
    <ContentPage.Content>
        <StackLayout>
            
            <Button x:Name="EditMeals"
                    Text="Edit Meals"
                    Clicked="EditMeals_OnClicked"
                    />

            <Button x:Name="EditStools"
                    Text="Edit Stools"
                    Clicked="EditStools_OnClicked"
                    />
            
            <Button x:Name="EditPains"
                    Text="Edit Pains"
                    Clicked="EditPains_OnClicked"
                    />

        </StackLayout>
    </ContentPage.Content>
</ContentPage>